{
  "printWidth": 120,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "jsxSingleQuote": true,
  "bracketSameLine": false,
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^react(.*)",
    "^next(.*)",
    "^@",
    "^clsx(.*)",
    "<THIRD_PARTY_MODULES>",
    "^store",
    "context/",
    "config",
    "utils/",
    "hooks/",
    "(components/|./index)",
    ".svg",
    "^../(.*)$",
    "^./(.*)$",
    "types",
    "^@remixicon/react",
    "(?=.module.scss)",
    "(?=.scss)",
    ".json"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
